                  +e.P.excerpt It is clear that over 13,000 banking customers&hellip;
                +e.A.more(href="#")
                  +icon('arrow-right', 'news__more-icon')
          
          .text-center
            .section-carousel-helpers
            
              .section-carousel-pagination.js-news-pagination

              .section-carousel-prev.js-news-prev
                +icon('arrow-left', 'section-carousel-prev__icon')
              .section-carousel-next.js-news-next
                +icon('arrow-right', 'section-carousel-next__icon')
  // /news-section

  // contacts-section
  section.promo-section.contacts-section

    .promo-section__container

      .row

        .col-md-7

          .entry
            .entry__heading Contact
              span.entry__desc Get in touch

          .callback-area
            .callback-area__desc We would like to hear from you with any questions you may have
            form.callback-area__form(action="/" method="POST" name="callback-form")
              .row
                .col-sm-6
                  .callback-area__field
                    .callback-area__field-main
                      +icon('email', 'callback-area__icon')
                      input.callback-area__input.input(type="email" name="user-email" placeholder="Your E-mail")
                .col-sm-6
                  .callback-area__field
                    .callback-area__field-main
                      +icon('user', 'callback-area__icon')
                      input.callback-area__input.input(type="text" name="user-name" placeholder="Your Name")
              .callback-area__field
                .callback-area__field-main
                  +icon('send', 'callback-area__icon')
                  textarea.callback-area__input.textarea(rows="1" cols="1" name="user-message" placeholder="Message")
              .text-right
                button.callback-area__btn.btn.btn--primary-reverse(type="submit") Send
                  +icon('arrow-right', 'btn__icon')

        .col-md-5
          +b.ASIDE.contacts-widget
            +e.main
              address.footer-contact
                .footer-contact__heading Office
                p.footer-contact__desc 4 / 5 Arran Square, Arran Quay, Dublin 7, Ireland.

              .footer-contact
                .footer-contact__heading Get in touch
                ul.footer-contact__list
                  li.footer-contact__item 
                    span Tel 
                    a(href="#") 01 872 5255
                  li.footer-contact__item 
                    span Fax
                    a(href="tel:+") 01 872 5489
                  li.footer-contact__item #[a(href="mailto:info@lawlorpartners.ie") info@lawlorpartners.ie]
              .footer-contact
                .footer-contact__heading Social
                +soc()
  // /contacts-section

