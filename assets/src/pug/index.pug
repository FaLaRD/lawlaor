extends templates/main

block variables
  - const title = 'Home page';
  - const pageClass = 'index-page';
  - const logoIndex = true;

block header
  +header(true)

  // intro
  section.promo-section.intro-section

    .promo-section__container

      .intro

        .entry
          .entry__heading About us
            span.entry__desc Solicitors &amp; Tax Consultants all under the one roof.

        .intro__wrap

          .intro__container

            .intro__hero-wrap
              .intro__hero
                img.intro__img(src="img/about-promo.jpg" alt="")
              .scroll-next.d-none.d-xl-block
                .scroll-next__desc Scroll down
                +icon('scroll', 'scroll-next__icon')

          .intro__info-main
            p.intro__desc Lawlor Kiernan LLP is a progressive legal and tax firm offering specialist advice. The practice was founded in 1995 and has established a reputation for the provision of high quality legal and tax services.

            a.intro__btn.btn(href="#") 
              span Learn More
              +icon('arrow-right', 'intro__btn-icon')

            .partner__details
              ul
                li 
                  a(href="tel:+018725255") 
                    +icon('phone-call')
                    span 01 872 5255
                li 
                  a(href="mailto:info@lawlorpartners.ie")
                    +icon('email')
                    span info@lawlorpartners.ie
  // /intro

  // advantages
  section.promo-section.advantages-section

    .promo-section__container

      .entry
        .entry__heading Practice Areas
          span.entry__desc We know how to defend your rights

      .container-fluid

        .swiper-container.section-carousel.js-advantages-carousel

          .swiper-wrapper

            .swiper-slide

              +b.A.advantage(href="#")
                +e.FIGURE.main
                  +e.img-wrap
                    +icon('shield', 'advantage__icon')
                  +e.FIGCAPTION.caption Data Protection

            .swiper-slide

              +b.A.advantage(href="#")
                +e.FIGURE.main
                  +e.img-wrap
                    +icon('money', 'advantage__icon')
                  +e.FIGCAPTION.caption Tracker Mortgage Overcharging

            .swiper-slide

              +b.A.advantage(href="#")
                +e.FIGURE.main
                  +e.img-wrap
                    +e.IMG.icon(src="img/broken-hand.png" alt="")
                  +e.FIGCAPTION.caption Personal Injury

            .swiper-slide

              +b.A.advantage(href="#")
                +e.FIGURE.main
                  +e.img-wrap
                    +icon('diploma', 'advantage__icon')
                  +e.FIGCAPTION.caption Probate

            .swiper-slide

              +b.A.advantage(href="#")
                +e.FIGURE.main
                  +e.img-wrap
                    +icon('help', 'advantage__icon')
                  +e.FIGCAPTION.caption Public Inquiries

            .swiper-slide

              +b.A.advantage(href="#")
                +e.FIGURE.main
                  +e.img-wrap
                    +icon('money', 'advantage__icon')
                  +e.FIGCAPTION.caption Tracker Mortgage Overcharging

            .swiper-slide

              +b.A.advantage(href="#")
                +e.FIGURE.main
                  +e.img-wrap
                    +icon('diploma', 'advantage__icon')
                  +e.FIGCAPTION.caption Probate

            .swiper-slide

              +b.A.advantage(href="#")
                +e.FIGURE.main
                  +e.img-wrap
                    +icon('help', 'advantage__icon')
                  +e.FIGCAPTION.caption Public Inquiries

            .swiper-slide

              +b.A.advantage(href="#")
                +e.FIGURE.main
                  +e.img-wrap
                    +icon('shield', 'advantage__icon')
                  +e.FIGCAPTION.caption Data Protection

            .swiper-slide

              +b.A.advantage(href="#")
                +e.FIGURE.main
                  +e.img-wrap
                    +e.IMG.icon(src="img/broken-hand.png" alt="")
                  +e.FIGCAPTION.caption Personal Injury

          .section-carousel-helpers

            .section-carousel-pagination.js-advantages-pagination

            .section-carousel-prev.js-advantages-prev
              +icon('arrow-left', 'section-carousel-prev__icon')
            .section-carousel-next.js-advantages-next
              +icon('arrow-right', 'section-carousel-next__icon')
  // /advantages

  // news-section
  section.promo-section.blog-section

    .promo-section__container

      .entry
        .entry__heading Personal
          span.entry__desc Blog 
            small / #[a(href="#") read more]
              i.fa.fa-long-arrow-right

      .container-fluid

        .swiper-container.section-carousel.js-news-carousel

          .swiper-wrapper

            .swiper-slide

              +b.ARTICLE.news
                +e.A.img-wrap(href="#")
                  +e.IMG.img(src="img/news-1.jpg" alt="news")
                +e.details
                  +e.meta
                    +e.TIME.date(datetime="11-07") Nov #[strong 07]
                    +e.viewed
                      +icon('eye', 'news__viewed-icon')
                      span 256
